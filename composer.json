{
    "name": "toxic-lemurs/menu-builder",
    "description": "A simple database driven menu manager for Laravel 6",
    "keywords": ["menu", "menu-builder", "html menus", "html", "menus", "eloquent", "laravel", "template", "database", "simple"],
    "homepage": "https://github.com/ToxicLemurs/MenuBuilder",
    "license": "MIT",
    "authors": [
        {
            "name": "Derik Nel",
            "email": "derik.nel@gmail.com",
            "homepage": "https://github.com/ToxicLemurs/MenuBuilder",
            "role": "Owner"
        }
    ],
    "require": {
        "php" : "^7.2",
        "illuminate/support": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "classmap": [
            "src/models",
            "src/Facades",
            "src/Http/Controllers"
        ],
        "psr-4": {
            "ToxicLemurs\\MenuBuilder\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
            "ToxicLemurs\\MenuBuilder\\MenuBuilderServiceProvider"
        ],
        "aliases": {
            "MenuBuilder": "ToxicLemurs\\MenuBuilder\\Facades\\MenuBuilder"
            }
        }
    }
}
